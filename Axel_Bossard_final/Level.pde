

class Level {
  float fblanc;
  float frouge;
  float vitesse;
  float vie;
  float fbonus;

  Level(float fblanc, float frouge, float vie, float fbonus, float vitesse) {
    this.fblanc=fblanc;
    this.frouge=frouge;
    this.vitesse=vitesse;
    this.vie=vie;
    this.fbonus=fbonus;
  }
}
